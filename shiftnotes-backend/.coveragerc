[run]
source = .
omit =
    */tests/*
    */test_*.py
    */migrations/*
    */venv/*
    */.venv/*
    */env/*
    */manage.py
    */wsgi.py
    */asgi.py
    */settings.py
    */conftest.py
    */setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

